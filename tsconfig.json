{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2015",
      "es2016.array.include"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5"
  }
}